{
    "name": "Text Vault",
    
    "description": "TextVault is a versatile Chrome extension that empowers you to effortlessly store and manage texts of any length.",
    "version":"1.0.0",
    "content_security_policy": {
        "extension_pages": "script-src 'self' ; object-src 'self'"
    },
    "action":{
        "default_popup":"index.html",
        "default_icon":"icons/logo_b.png"
    },
    "icons": {
        "16": "icons/logo_b.png",
        "32": "icons/logo_b.png",
        "48":"icons/logo_b.png",
        "128": "icons/logo_b.png"
      },
    "manifest_version":3,
    "permissions": [
        "clipboardRead",
        "clipboardWrite"
      ]
}
